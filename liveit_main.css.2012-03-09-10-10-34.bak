
#liveit {
 margin-left: -2em;
 font-size: 14px;
 
}
#liveit_edit {
 position: absolute;
 display: block;
 right: 4%;
 top: 20%;
 background-image: url(http://localhost:8888/wp3/wp-content/plugins/LIVEIT/icon_edit.png);
 background-attachment: initial;
 background-origin: initial;
 background-clip: initial;
 background-color: black;
 border-top-left-radius: 100px;
 border-top-right-radius: 100px;
 border-bottom-right-radius: 100px;
 border-bottom-left-radius: 100px;
 width: 50px;
 height: 50px;
 opacity: 0.4;
 
}
#liveit_edit img {
 display: block;
 position: absolute;
 top: 22%;
 left: 22%;
 
}
#liveit_save_button {
 display: block;
 position: relative;
 float: right;
 margin-right: 5%;
 margin-top: 5px;
 width: 100px;
 font-size: 16px;
 padding-bottom: 2px;
 line-height: 1.2em;
 
}
#liveit_edit:hover {
 cursor: pointer;
 opacity: 1;
 
}
#liveit_sidebar {
 display: none;
 position: fixed;
 width: 25%;
 right: 0%;
 top: 28px;
 background-image: initial;
 background-attachment: initial;
 background-origin: initial;
 background-clip: initial;
 background-color: rgb(76, 76, 76);
 color: rgb(255, 189, 0);
 list-style-type: none;
 list-style-position: initial;
 list-style-image: initial;
 height: 95%;
 overflow-y: scroll;
 
}
#liveit_bottom {
 width: 100%;
 position: fixed;
 bottom: 0px;
 height: 5%;
 min-height: 30px;
 background-image: initial;
 background-attachment: initial;
 background-origin: initial;
 background-clip: initial;
 background-color: black;
 z-index: 1000000;
 visibility: visible;
 display: none;
 
}
#liveit_bottom .liveit_bottom_close {
 display: block;
 position: absolute;
 right: 20px;
 top: 5px;
 height: 24px;
 width: 24px;
 background-image: url(http://localhost:8888/wp3/wp-content/plugins/LIVEIT/close.png);
 background-attachment: initial;
 background-origin: initial;
 background-clip: initial;
 background-color: initial;
 cursor: pointer;
 opacity: 0.6;
 background-position: initial initial;
 background-repeat: initial initial;
 
}
.liveit_bottom_close {
 opacity: 1;
 
}
.border {
 border-top-width: 2px;
 border-right-width: 2px;
 border-bottom-width: 2px;
 border-left-width: 2px;
 border-top-color: rgb(255, 0, 128);
 border-right-color: rgb(255, 0, 128);
 border-bottom-color: rgb(255, 0, 128);
 border-left-color: rgb(255, 0, 128);
 border-top-style: solid;
 border-right-style: solid;
 border-bottom-style: solid;
 border-left-style: solid;
 border-image: initial;
 
}
#liveit_sidebar .prop_name {
 color: rgb(179, 179, 179);
 position: relative;
 display: inline;
 
}
#liveit_notify_spinner {
 position: absolute;
 display: block;
 right: 10%;
 bottom: 20%;
 
}
white #liveit_sidebar .title {
 display: block;
 clear: both;
 background-image: initial;
 background-attachment: initial;
 background-origin: initial;
 background-clip: initial;
 background-color: rgb(51, 51, 51);
 color: rgb(255, 206, 2);
 padding-top: 10px;
 padding-right: 10px;
 padding-bottom: 10px;
 padding-left: 10px;
 
}
#liveit_sidebar .prop_val {
 display: inline;
 color: rgb(255, 189, 0);
 position: relative;
 
}
.stylesheet_name {
 display: block;
 clear: both;
 position: relative;
 background-image: initial;
 background-attachment: initial;
 background-origin: initial;
 background-clip: initial;
 background-color: rgb(51, 51, 51);
 font-size: 14px;
 color: rgb(230, 230, 230);
 margin-top: 0px;
 margin-right: 0px;
 margin-bottom: 0px;
 margin-left: 0px;
 padding-top: 10px;
 padding-right: 10px;
 padding-bottom: 10px;
 padding-left: 10px;
 
}
.selector_name {
 color: rgb(0, 128, 255);
 
}
#liveit_sidebar ul {
 list-style-type: none;
 list-style-position: initial;
 list-style-image: initial;
 margin-top: 0px;
 margin-right: 0px;
 margin-bottom: 0px;
 margin-left: 0px;
 
}
#liveit_head {
 height: 50px;
 width: 100%;
 display: block;
 position: relative;
 background-image: initial;
 background-attachment: initial;
 background-origin: initial;
 background-clip: initial;
 background-color: black;
 
}
.selector {
 float: left;
 margin-left: 10px;
 margin-top: 10px;
 opacity: 0.1;
 display: block;
 position: relative;
 
}
.selected {
 opacity: 1;
 
}
#toggle {
 margin-left: 50px;
 display: block;
 position: relative;
 float: left;
 padding-top: 5px;
 padding-right: 10px;
 padding-bottom: 5px;
 padding-left: 10px;
 margin-top: 10px;
 
}
.btn {
 display: inline-block;
 padding-top: 4px;
 padding-right: 10px;
 padding-bottom: 4px;
 padding-left: 10px;
 margin-bottom: 0px;
 font-size: 13px;
 line-height: 18px;
 color: rgb(51, 51, 51);
 text-align: center;
 vertical-align: middle;
 background-color: rgb(245, 191, 0);
 border-top-left-radius: 4px;
 border-top-right-radius: 4px;
 border-bottom-right-radius: 4px;
 border-bottom-left-radius: 4px;
 -webkit-box-shadow: rgba(255, 255, 255, 0.199219) 0px 1px 0px inset, rgba(0, 0, 0, 0.0429688) 0px 1px 2px;
 box-shadow: rgba(255, 255, 255, 0.199219) 0px 1px 0px inset, rgba(0, 0, 0, 0.0429688) 0px 1px 2px;
 cursor: pointer;
 background-repeat: repeat no-repeat;
 
}
.btn:hover, .btn:active, .btn.active, .btn.disabled, .btn[disabled] {
 background-color: rgb(230, 230, 230);
 
}
.btn:active, .btn.active {
 
}
.btn:first-child {
 
}
.btn:hover {
 color: rgb(51, 51, 51);
 text-decoration: none;
 background-color: rgb(230, 227, 0);
 -webkit-transition-property: background-position;
 -webkit-transition-duration: 0.1s;
 -webkit-transition-timing-function: linear;
 -webkit-transition-delay: initial;
 background-position: 0px -15px;
 
}
.btn:focus {
 outline-width: 5px;
 outline-style: auto;
 outline-color: -webkit-focus-ring-color;
 outline-offset: -2px;
 
}
#liveit_bottom .liveit_notify {
 display: none;
 position: absolute;
 bottom: 50px;
 right: 50px;
 border-top-left-radius: 10px;
 border-top-right-radius: 10px;
 border-bottom-right-radius: 10px;
 border-bottom-left-radius: 10px;
 width: 600px;
 background-image: initial;
 background-attachment: initial;
 background-origin: initial;
 background-clip: initial;
 background-color: rgb(254, 181, 0);
 padding-top: 10px;
 padding-right: 10px;
 padding-bottom: 10px;
 padding-left: 10px;
 
}
.up-arrow {
